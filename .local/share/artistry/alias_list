alias lsf='/bin/ls --quoting-style=literal -Alph --time-style=iso --color=auto --group-directories-first --show-control-chars --hyperlink'
alias diff='/bin/diff --color=auto'
alias grep='/bin/grep --color=auto'
alias egrep='/bin/egrep --color=auto'
alias fgrep='/bin/fgrep --color=auto'
alias sudo='sudo '
alias pm='sudo pacman'
alias pmin='sudo pacman -S'
alias ":q"='exit'
alias ":Q"='exit'
alias gitpom='ghpwd && git push'
alias gits='git status'
alias gitca='git commit -a -m'
alias cls='clear && neofetch'
